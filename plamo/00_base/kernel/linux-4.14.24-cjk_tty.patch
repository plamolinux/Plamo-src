+static ssize_t store_bind(struct device *dev, struct device_attribute *attr,
+			  const char *buf, size_t count)
+{
+	struct con_driver *con = dev_get_drvdata(dev);
+	int bind = simple_strtoul(buf, NULL, 0);
+
+	console_lock();
+
+	if (bind)
+		vt_bind(con);
+	else
+		vt_unbind(con);
+
+	console_unlock();
+
+	return count;
+}
