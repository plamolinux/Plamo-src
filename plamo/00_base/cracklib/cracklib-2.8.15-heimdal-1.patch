+char
+Chop(string)
+    register char *string;
+{
+    register char c;
+    register char *ptr;
+    c = '\0';
+
+    for (ptr = string; *ptr; ptr++);
+    if (ptr != string)
+    {
+	c = *(--ptr);
+	*ptr = '\0';
+    }
+    return (c);
+}
+
+char *
+Trim(string)
+    register char *string;
+{
+    register char *ptr;
+    for (ptr = string; *ptr; ptr++);
+
+    while ((--ptr >= string) && isspace(*ptr));
+
+    *(++ptr) = '\0';
+
+    return (ptr);
+}
+
+char *
+Clone(string)
+    char *string;
+{
+    register char *retval;
+    retval = (char *) malloc(strlen(string) + 1);
+    if (retval)
+    {
+	strcpy(retval, string);
+    }
+    return (retval);
+}
